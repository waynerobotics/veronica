<!-- ### This launch file is enables auto drive with any node that publishes cmd_vel  
(same as manual_drive but does not launch rqt_steering node)### -->
<!-- ### Requires encoder ticks published to topics lwheel (std_msgs/Int16) && rwheel (std_msgs/Int16)  ### -->
<!-- ### publishes wheel motor pwm values motor controller arduino should subscribe to -->

<launch>


<node pkg="urg_node" type="urg_node" name="urg_node" >    
    <!--param name="port" value="dev/veronica/laser_hokuyo" /-->
    <!--param name="port" value="dev/ttyACM0" /-->
</node>
<node pkg="tf" type="static_transform_publisher" name="tf_base_link_laser" args="0 0 0 0 0 0 base_link laser 10" />    


<!-- #############    Localization  ##############-->
<node pkg="localization" type="encoder_odom_publisher" name="encoder_odom_publisher" />    

<!-- #############    Drive and Motor Controllers  ##############-->
 <include file="$(find veronica)/launch/base_drive.launch" />



</launch>
