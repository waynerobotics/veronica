<robot name = "robot_model">
	<gazebo>
		<plugin name="gazebo_ros_control" filename="libgazebo_ros_control.so">
			<robotNamespace>wayne_robotics_model</robotNamespace>
			<legacyModeNS>true</legacyModeNS>
		</plugin>
	</gazebo>

	<material name="rubber">
		<color rgba="0 0 0 1"/>
	</material>

	<link name = "base_link">
		<visual>
			<origin rpy="0 0 0" xyz="-0.257 0 0"/>
			<geometry>
				<box size="0.914 0.6069 0.2159"/>
			</geometry>
		</visual>
		<collision>
			<origin rpy="0 0 0" xyz="-0.257 0 0"/>
			<geometry>
				<box size="0.914 0.6069 0.2159"/>
			</geometry>
		</collision>
		<inertial>
			<origin rpy="0 0 0" xyz="-0.257 0 0"/>
			<mass value="100"/>
			<inertia ixx="3.485" ixy="0" ixz="0" iyy="7.35" iyz="0" izz="10.058"/>
		</inertial>
	</link>


	<link name = "left_wheel">
		<visual name="left_wheel_visual">
			<origin rpy="1.5707 0 0" xyz="0 0 0"/>
			<geometry>
				<cylinder length="0.1524" radius="0.2"/>
			</geometry>
			<material name="rubber"/>
		</visual>
		<collision>
			<origin rpy="1.5707 0 0" xyz="0 0 0"/>
			<geometry>
				<cylinder length="0.1524" radius="0.2"/>
			</geometry>
		</collision>
		<inertial>
			<origin rpy="1.5707 0 0" xyz="0 0 0"/>
			<mass value="10"/>
			<inertia ixx="0.11935" ixy="0" ixz="0" iyy="0.11935" iyz="0" izz="0.2"/>
		</inertial>
	</link>
	<joint name="left_wheel_joint" type="continuous">
		<origin xyz="0 0.4445 -0.10795"/>
		<axis xyz="0 1 0"/>
		<parent link="base_link"/>
		<child link="left_wheel"/>
	</joint>
	<transmission name="left_wheel">
		<type> transmission_interface/SimpleTransmission</type>
		<joint name="left_wheel_joint">
			<hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
		</joint>
		<actuator name="left_motor">
			<mechanicalReduction>1</mechanicalReduction>
		</actuator>
	</transmission>

	<link name = "right_wheel">
		<visual name="right_wheel_visual">
			<origin rpy="1.5707 0 0" xyz="0 0 0"/>
			<geometry>
				<cylinder length="0.1524" radius="0.2"/>
			</geometry>
			<material name="rubber"/>
		</visual>
		<collision>
			<origin rpy="1.5707 0 0" xyz="0 0 0"/>
			<geometry>
				<cylinder length="0.1524" radius="0.2"/>
			</geometry>
		</collision>
		<inertial>
			<origin rpy="1.5707 0 0" xyz="0 0 0"/>
			<mass value="10"/>
			<inertia ixx="0.11935" ixy="0" ixz="0" iyy="0.11935"  iyz="0" izz="0.2"/>
		</inertial>
	</link>
	<joint name="right_wheel_joint" type="continuous">
		<origin xyz="0 -0.4445 -0.10795"/>
		<axis xyz="0 1 0"/>
		<parent link="base_link"/>
		<child link="right_wheel"/>
	</joint>
	<transmission name="right_wheel">
		<type>transmission_interface/SimpleTransmission</type>
		<joint name="right_wheel_joint">
			<hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
		</joint>
		<actuator name="right_motor">
			<mechanicalReduction>1</mechanicalReduction>
		</actuator>
	</transmission>

	<link name = "rear_stand">
		<visual name="rear_stand_visual">
			<origin rpy="0 0 0" xyz="0 0 0"/>
			<geometry>
				<cylinder length="0.15" radius="0.05"/>
			</geometry>
		</visual>
		<collision>
			<origin rpy="0 0 0" xyz="0 0 0"/>
			<geometry>
				<cylinder length="0.15" radius="0.05"/>
			</geometry>
		</collision>
		<inertial>
			<origin rpy="0 0 0" xyz="0 0 0"/>
			<mass value="1"/>
			<inertia ixx="0.0030625" ixy="0" ixz="0" iyy="0.0030625"  iyz="0" izz="0.00125"/>
		</inertial>
	</link>
	<joint name="rear_stand_joint" type="continuous">
		<origin xyz="-0.6 0 -0.18295"/>
		<axis xyz="0 0 1"/>
		<parent link="base_link"/>
		<child link="rear_stand"/>
	</joint>	

	<link name = "rear_stand_wheel">
		<visual name="rear_stand_wheel_visual">
			<origin rpy="1.5707 0 0" xyz="0 0 0"/>
			<geometry>
				<cylinder length="0.076200" radius="0.05"/>
			</geometry>
			<material name="rubber"/>
		</visual>
		<collision>
			<origin rpy="1.5707 0 0" xyz="0 0 0"/>
			<geometry>
				<cylinder length="0.076200" radius="0.05"/>
			</geometry>
		</collision>
		<inertial>
			<origin rpy="1.5707 0 0" xyz="0 0 0"/>
			<mass value="1"/>
			<inertia ixx="0.0011089" ixy="0" ixz="0" iyy="0.0011089" iyz="0" izz="0.00125"/>
		</inertial>
	</link>
	<joint name="rear_stand_wheel_joint" type="continuous">
		<origin xyz="-0.05 0 -0.075"/>
		<axis xyz="0 1 0"/>
		<parent link="rear_stand"/>
		<child link="rear_stand_wheel"/>
	</joint>



	<gazebo reference="left_wheel">
		<collision>
			<surface>
				<friction>
					<ode>
						<mu>1000</mu>
						<mu2>1000</mu2>
					</ode>
				</friction>
			</surface>
		</collision>
	</gazebo>

	<gazebo reference="right_wheel">
		<collision>
			<surface>
				<friction>
					<ode>
						<mu>1000</mu>
						<mu2>1000</mu2>
					</ode>
				</friction>
			</surface>
		</collision>
	</gazebo>
	<gazebo reference="rear_stand_wheel">
		<collision>
			<surface>
				<friction>
					<ode>
						<mu>1000</mu>
						<mu2>1000</mu2>
					</ode>
				</friction>
			</surface>
		</collision>
	</gazebo>
</robot>
