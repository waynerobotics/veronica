All Rights Reserved

Email: contact@neilnie.com

-->

<launch>

    <include file="/home/warriorrobotics/veronica/src/zed-ros-wrapper/zed_wrapper/launch/zed2.launch" />
    <!-- Start RTAB-map launch file, localization mode -->
    <include file="$(find veronica_rtab)/launch/rtabmap_localization.launch" />

    <remap from="map" to="/rtabmap/grid_map" />
    <remap from="move_base/current_goal" to="move_base_simple/goal"/>

    <!-- Launch the move_base node with the specified config files -->
    <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">

        <param name="base_global_planner" value="global_planner/GlobalPlanner" />

        <rosparam file="$(find veronica_rtab)/param/costmap_common_params.yaml"       command="load" ns="global_costmap" />
        <rosparam file="$(find veronica_rtab)/param/costmap_common_params.yaml"       command="load" ns="local_costmap" />
        <rosparam file="$(find veronica_rtab)/param/local_costmap_params.yaml"        command="load" />
        <rosparam file="$(find veronica_rtab)/param/global_costmap_params.yaml"       command="load" />
        <rosparam file="$(find veronica_rtab)/param/base_local_planner.yaml"   command="load" />
        <rosparam file="$(find veronica_rtab)/param/global_planner_params.yaml"       command="load" />

    </node>

    <!--node pkg="motion_control" type="motion_control_node" name="motion_control_node" >

        <param name="angle_scale" value="-10.5" />

    </node-->

    <!-- Receive goals using websocket and iOS user interface -->
    <!-- <node pkg="path_planning" name="navigation_goals" type="navigation_goals" output="screen" /> -->

</launch>